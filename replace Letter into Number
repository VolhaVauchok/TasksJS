function vowelOne(s){
  /*let str = s.replace(/[^aeiou]/gi, '0');
  let strr = str.replace(/[^0]/gi, '1');*/
  return s.replace(/[^aeiou]/gi, '0').replace(/[^0]/gi, '1');
}
